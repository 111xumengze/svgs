{
  "name": "svgs",
  "version": "2.0.0",
  "description": "svgs is a compatiblity layer between svg and react-native-svg",
  "scripts": {
    "test": "mocha --require enzyme/withDom --compilers js:babel-register test.js",
    "lint": "godaddy-js-style-lint index.js index.native.js test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/svgs"
  },
  "mochify": {
    "transform": "babelify",
    "reporter": "spec",
    "ui": "bdd"
  },
  "keywords": [
    "svg",
    "react-native",
    "svgs",
    "compatiblity",
    "element",
    "react"
  ],
  "dependencies": {
    "babel-preset-es2015": "6.22.x",
    "babel-preset-react": "6.23.x",
    "babelify": "7.3.x",
    "rip-out": "1.0.x"
  },
  "peerDependencies": {
    "react": "15.x.x"
  },
  "devDependencies": {
    "assume": "1.4.x",
    "babel-register": "6.23.x",
    "enzyme": "2.7.x",
    "godaddy-style": "3.1.x",
    "jsdom": "9.11.x",
    "mocha": "3.2.x",
    "react": "15.x.x",
    "react-addons-test-utils": "15.x.x",
    "react-dom": "15.4.x",
    "react-native-svg": "5.x.x"
  },
  "author": "GoDaddy Operating Company, LLC",
  "contributors": [
    "Arnout Kazemier"
  ],
  "license": "MIT",
  "babel": {
    "presets": [
      "babel-preset-es2015",
      "babel-preset-react"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
